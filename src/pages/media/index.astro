    <section class="container-e5 py-12">
        <div class="mx-auto">
            <h2 class="text-e5-red text-[36px] font-['Franklin_Gothic_Book'] tracking-[7.2px] uppercase mb-8">
                DAYTONA</h2>
            <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-8">
                <MediaDownload alt="Talladega Wheel"
                               image="https://ml3bwkhva5az.i.optimole.com/cb:c_IM.4ce03/w:1500/h:1500/q:100/rt:fill/g:ce/sm:0/ig:avif/https://e5wheels.com/wp-content/uploads/2025/06/Talladega_AB_Brushed_Aluminum_5-lug_0001.webp"
                               downloadLink="https://e5wheels.com/wp-content/uploads/2025/07/E5_Talledega.zip"/>
            </div>
        </div>
    </section>
</MainLayout>